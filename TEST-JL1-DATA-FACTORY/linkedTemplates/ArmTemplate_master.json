{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nazwa fabryki danych",
			"defaultValue": "TEST-JL1-DATA-FACTORY"
		},
		"J_LABS_BI_connectionString": {
			"type": "secureString",
			"metadata": "Bezpieczny ciąg dla elementu „connectionString” — „J_LABS_BI”"
		},
		"J_LABS_BI_password": {
			"type": "secureString",
			"metadata": "Bezpieczny ciąg dla elementu „password” — „J_LABS_BI”"
		},
		"SharePoint_JLabs_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Bezpieczny ciąg dla elementu „servicePrincipalKey” — „SharePoint_JLabs”"
		},
		"J_LABS_BI_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "J-LABS\\witold.toczek.ext"
		},
		"SharePoint_JLabs_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "8f530e7c-b811-4d45-a09f-dd3c20139862"
		},
		"containerUri": {
			"type": "string",
			"metadata": "Identyfikator URI konta magazynu zawierającego połączone szablony usługi ARM"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token SAS dla konta magazynu zawierającego połączone szablony usługi ARM"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "TEST-JL1-DATA-FACTORY_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SharePoint_JLabs_servicePrincipalKey": {
						"value": "[parameters('SharePoint_JLabs_servicePrincipalKey')]"
					},
					"J_LABS_BI_connectionString": {
						"value": "[parameters('J_LABS_BI_connectionString')]"
					},
					"J_LABS_BI_password": {
						"value": "[parameters('J_LABS_BI_password')]"
					},
					"SharePoint_JLabs_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('SharePoint_JLabs_properties_typeProperties_servicePrincipalId')]"
					},
					"J_LABS_BI_properties_typeProperties_userName": {
						"value": "[parameters('J_LABS_BI_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}